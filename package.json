{
  "name": "bulkie.js",
  "version": "0.0.34",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "bun run postinstall.config.ts",
    "prebuild": "bun run build:lit-actions",
    "prepublishOnly": "bun run version.config.ts && bun run build",
    "dev": "bun run prebuild && bun run dev.ts",
    "build:lit-actions": "bun run ./esbuild.config.ts",
    "build": "concurrently \"bun run build:cjs\" \"bun run build:esm\" \"bun run build:types\" \"bun run build:umd\"",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "build:umd": "webpack --config webpack.config.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@useorbis/db-sdk": "0.0.60-alpha",
    "ethers": "5.7.2",
    "openai": "^4.68.4",
    "typestub-ipfs-only-hash": "^4.0.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  },
  "peerDependencies": {
    "@lit-protocol/accs-schemas": "^0.0.15",
    "@lit-protocol/constants": "^7.0.0-alpha.9",
    "@lit-protocol/contracts-sdk": "^7.0.0-alpha.9",
    "@lit-protocol/lit-node-client": "^7.0.0-alpha.9",
    "@lit-protocol/types": "^7.0.0-alpha.9",
    "@lit-protocol/wrapped-keys": "^6.11.0"
  },
  "engines": {
    "bun": ">=0.6.0"
  },
  "devDependencies": {
    "@lit-protocol/accs-schemas": "0.0.15",
    "@lit-protocol/constants": "7.0.0-alpha.9",
    "@lit-protocol/contracts-sdk": "7.0.0-alpha.9",
    "@lit-protocol/lit-node-client": "7.0.0-alpha.9",
    "@lit-protocol/types": "7.0.0-alpha.9",
    "@lit-protocol/wrapped-keys": "^6.11.0",
    "@types/jszip": "^3.4.0",
    "buffer": "^6.0.3",
    "concurrently": "^9.0.1",
    "crypto-browserify": "^3.12.1",
    "esbuild": "^0.24.0",
    "stream-browserify": "^3.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "util": "^0.12.5",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}